<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.optimealapp"
    android:versionCode="1"
    tools:ignore="GradleOverrides"
    android:versionName="1.0">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@drawable/logooo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logooo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".ChangeMealActivity"
            android:label="Modifications"
            android:parentActivityName=".RecipeActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".GenerateActivity"
            android:label="Générer des menus"
            android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".RecipeActivity"
            android:label="Recette"
            android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".InscriptionActivity"
            android:label="Inscription"
            android:parentActivityName=".LogInActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".LogInActivity"
            android:label="Connexion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LivraisonActivity"
            android:label="Livraison"
            android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".AddPublicationActivity"
            android:label="Partager un plat"
            android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>